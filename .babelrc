{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "Chrome >= 59",
            "FireFox >= 44",
            "Safari >= 7",
            "last 4 Edge versions"
          ]
        },
        "useBuiltIns": "entry",
        "corejs": 2
      }
    ]
  ],
  "plugins": [ "@babel/plugin-proposal-object-rest-spread", 
    "@babel/plugin-transform-arrow-functions" ]
}
