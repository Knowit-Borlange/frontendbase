{
  "extends": "airbnb",
  "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": 0,
      "react/no-deprecated": 0,
      "react/jsx-filename-extension": 0,
      "no-unused-vars": 0,
      "object-curly-spacing": 0,
      "no-var": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "react/jsx-indent": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-quotes": 0,
      "react/prop-types": 0,
      "react/prefer-es6-class": 0,
      "react/prefer-stateless-function": 0,
      "object-shorthand": 0,
      "func-names": 0,
      "comma-dangle": 0,
      "keyword-spacing": 0,
      "no-console": 0
    }
}
